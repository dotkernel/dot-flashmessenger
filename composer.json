{
  "name": "dotkernel/dot-flashmessenger",
  "type": "library",
  "description": "Flash messenger library for session messages between redirects",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "psr/http-message": "^1.0",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-servicemanager": "^3.0",

    "dotkernel/dot-session": "0.6.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "DotKernel\\DotFlashMessenger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotKernelTest\\DotFlashMessenger\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-session"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
