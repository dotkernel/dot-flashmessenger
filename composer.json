{
  "name": "dotkernel/dot-flashmessenger",
  "type": "library",
  "description": "DotKernel flash messenger component for session messages between redirects",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-servicemanager": "^3.3.0",

    "dotkernel/dot-session": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "zendframework/zend-expressive-template": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Dot\\FlashMessenger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\FlashMessenger\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev",
      "dev-develop": "1.1-dev"
    }
  }
}
