{
    "name": "dotkernel/dot-flashmessenger",
    "type": "library",
    "description": "DotKernel flash messenger component for session messages between redirects",
    "keywords": [
        "mezzion",
        "flashmessenger",
        "laminas"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "psr/http-message": "^1.0.1",
        "laminas/laminas-servicemanager": "^3.4.0",
        "dotkernel/dot-session": "^4.0.0",
        "laminas/laminas-dependency-plugin": "^1.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1.3",
        "squizlabs/php_codesniffer": "^3.5.5",
        "mezzio/mezzio-template": "^2.0.1"
    },
    "autoload": {
        "psr-4": {
            "Dot\\FlashMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\FlashMessenger\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev",
            "dev-develop": "4.1-dev"
        }
    }
}
